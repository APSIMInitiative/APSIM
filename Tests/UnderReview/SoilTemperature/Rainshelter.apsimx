                        "",
                        "        [Description(\"FodderBeet planting date: \")]",
                        "        public DateTime FodderBeetPlantingDate { get; set; }",
                        "        [Description(\"FodderBeet harvesting date: \")]",
                        "        public DateTime FodderBeetHarvestingDate { get; set; }",
                        "        [Description(\"Oats planting date: \")]",
                        "        public DateTime OatsPlantingDate { get; set; }",
                        "        [Description(\"Oats harvesting date 1: \")]",
                        "        public DateTime OatsHarvestingDate1 { get; set; }",
                        "        [Description(\"Oats harvesting date 2: \")]",
                        "        public DateTime OatsHarvestingDate2 { get; set; }",
                        "        ",
                        "        [JsonIgnore] public string CropSown { get; set; }",
                        "        [JsonIgnore] public string CropAction { get; set; }",
                        "",
                        "        [JsonIgnore] public bool IrrgFactor { get; set; }",
                        "        [JsonIgnore] public double FertFactor { get; set; }",
                        "",
                        "",
                        "        ",
                        "        [EventSubscribe(\"StartOfSimulation\")]",
                        "        private void OnSimulationCommencing(object sender, EventArgs e)",
                        "        {",
                        "            CropSown = \"Fallow\";",
                        "            CropAction = \"-\";",
                        "        }",
                        "        ",
                        "        [EventSubscribe(\"DoManagement\")]",
                        "        private void OnDoManagement(object sender, EventArgs e)",
                        "        {",
                        "            CropAction = \"-\";",
                        "",
                        "            if (Clock.Today.Date == FodderBeetPlantingDate)",
                        "            {",
                        "                FodderBeet.Sow(population: 9, cultivar: \"Rivage\", depth: 5, rowSpacing: 500);",
                        "                CropSown = \"FodderBeet\";",
                        "                CropAction = \"Sowing\";",
                        "            }",
                        "            ",
                        "            if (Clock.Today.Date == OatsPlantingDate)",
                        "            {",
                        "                Oats.Sow(population: 400, cultivar: \"Milton\", depth: 45, rowSpacing: 45);",
                        "                CropSown = \"Oats\";",
                        "                CropAction = \"Sowing\";",
                        "            }",
                        "            ",
                        "            ",
                        "            if (Clock.Today.Date == FodderBeetHarvestingDate)",
                        "            {",
                        "                HarvestFodderBeet();",
                        "                //FodderBeet.EndCrop();",
                        "                //CropSown = \"Fallow\";",
                        "                CropAction = \"Harvesting\";",
                        "            }",
                        "            if (Clock.Today.Date == FodderBeetHarvestingDate.AddDays(1))",
                        "            {",
                        "                //FodderBeet.Harvest(removalFractions);",
                        "                FodderBeet.EndCrop();",
                        "                CropSown = \"Fallow\";",
                        "            }",
                        "",
                        "            if (Clock.Today.Date == OatsHarvestingDate1)",
                        "            {",
                        "               if((FertFactor == 0) || ((IrrgFactor == false) && (FertFactor == 50)))",
                        "               {",
                        "                   HarvestOats();",
                        "                   CropAction = \"Harvesting\";",
                        "               }",
                        "            }",
                        "",
                        "            if (Clock.Today.Date == OatsHarvestingDate2)",
                        "            {",
                        "               if((FertFactor == 300) || ((IrrgFactor == true) && (FertFactor == 50)))",
                        "               {",
                        "                   HarvestOats();",
                        "                   CropAction = \"Harvesting\";",
                        "               }",
                        "            }",
                        "",
                        "            if (Clock.Today.Date == OatsHarvestingDate1.AddDays(1))",
                        "            {",
                        "               if((FertFactor == 0) || ((IrrgFactor == false) && (FertFactor == 50)))",
                        "               {",
                        "                   Oats.EndCrop();",
                        "                   CropSown = \"Fallow\";",
                        "               }",
                        "            }",
                        "",
                        "            if (Clock.Today.Date == OatsHarvestingDate2.AddDays(1))",
                        "            {",
                        "               if((FertFactor == 300) || ((IrrgFactor == true) && (FertFactor == 50)))",
                        "               {",
                        "                   Oats.EndCrop();",
                        "                   CropSown = \"Fallow\";",
                        "               }",
                        "            }",
                        "        }",
                        "        ",
                        "        private void HarvestFodderBeet()",
                        "        {",
                        "            var Leaf = FodderBeet.FindChild<IHasDamageableBiomass>(\"Leaf\");",
                        "            var Petiole = FodderBeet.FindChild<IHasDamageableBiomass>(\"Petiole\");",
                        "            var StorageRoot = FodderBeet.FindChild<IHasDamageableBiomass>(\"StorageRoot\");",
                        "            ",
                        "            Leaf.RemoveBiomass(liveToRemove: 0.95, liveToResidue: 0.05, deadToRemove: 0.8, deadToResidue: 0.2);",
                        "            Petiole.RemoveBiomass(liveToRemove: 1.0, liveToResidue: 0.0, deadToRemove: 0.9, deadToResidue: 0.1);",
                        "            StorageRoot.RemoveBiomass(liveToRemove: 1.0, liveToResidue: 0.0, deadToRemove: 1.0, deadToResidue: 0.0);",
                        "             FodderBeet.Harvest(removeBiomassFromOrgans: false);",
                        "        }",
                        "        ",
                        "        private void HarvestOats()",
                        "        {",
                        "            var Stem = Oats.FindChild<IHasDamageableBiomass>(\"Stem\");",
                        "            var Panicle = Oats.FindChild<IHasDamageableBiomass>(\"Panicle\");",
                        "            var Grain = Oats.FindChild<IHasDamageableBiomass>(\"Grain\");",
                        "            ",
                        "            Stem.RemoveBiomass(liveToRemove: 0.9, liveToResidue: 0.1, deadToRemove: 0.8, deadToResidue: 0.2);",
                        "            Panicle.RemoveBiomass(liveToRemove: 0.9, liveToResidue: 0.1, deadToRemove: 0.8, deadToResidue: 0.2);",
                        "            Grain.RemoveBiomass(liveToRemove: 1.0, liveToResidue: 0.0, deadToRemove: 1.0, deadToResidue: 0.0);",
                        "             Oats.Harvest(removeBiomassFromOrgans: false);",
                        "        }  ",
                        "    }",
                        "}"
