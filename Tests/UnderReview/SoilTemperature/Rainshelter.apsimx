                        "        [EventSubscribe(\"DoManagement\")]",
                        "        private void OnDoManagement(object sender, EventArgs e)",
                        "        {",
                        "            // Fodderbeet - irrigation on high fert plots only",
                        "            if ((IrrFactor == true) && (NitFactor == 300))",
                        "            {",
                        "                if (Clock.Today.Date == DateTime.Parse(\"01-Feb-2017\"))",
                        "                    Irrigation.Apply(40.0);",
                        "                if (Clock.Today.Date == DateTime.Parse(\"01-Mar-2017\"))",
                        "                    Irrigation.Apply(32.6);",
                        "            }",
                        "        ",
                        "            // Oats - Irrigation on dryland plots only, after harvest",
                        "            if (IrrFactor == false)",
                        "            {",
                        "                if ((Clock.Today.Date == DateTime.Parse(\"25-May-2017\")) || (Clock.Today.Date == DateTime.Parse(\"26-May-2017\")))",
                        "                    Irrigation.Apply(50.0);",
                        "",
                        "                if (Clock.Today.Date == DateTime.Parse(\"29-May-2017\"))",
                        "                if (NitFactor == 300)",
                        "                    Irrigation.Apply(64.0);",
                        "",
                        "                if (Clock.Today.Date == DateTime.Parse(\"30-May-2017\"))",
                        "                {",
                        "                    if (NitFactor == 0)",
                        "                        Irrigation.Apply(30.3);",
                        "                    if (NitFactor == 50)",
                        "                        Irrigation.Apply(51.0);",
                        "                }",
                        "            }",
                        "",
                        "            // Oats - Irrigation on all 300N plots plus dryland 50N plots",
                        "            if (Clock.Today.Date == DateTime.Parse(\"13-Dec-2017\"))",
                        "            {",
                        "                if (NitFactor == 300)",
                        "                    Irrigation.Apply(50.0);",
                        "                if ((NitFactor == 50) && (IrrFactor == false))",
                        "                    Irrigation.Apply(50.0);",
                        "            }",
                        "",
                        "            // Oats - Fertiliser for irrigated plots only, on sowing",
                        "            if (Clock.Today.Date == DateTime.Parse(\"05-Jul-2017\"))",
                        "            if (IrrFactor == true)",
                        "            {",
                        "                if (NitFactor == 0)",
                        "                    Fert.Apply(40, Models.Fertiliser.Types.UreaN, 10);",
                        "                if (NitFactor == 50)",
                        "                    Fert.Apply(85, Models.Fertiliser.Types.UreaN, 10);",
                        "                if (NitFactor == 300)",
                        "                    Fert.Apply(270, Models.Fertiliser.Types.UreaN, 10);",
                        "            }",
                        "        }",
                        "    }",
                        "}"
