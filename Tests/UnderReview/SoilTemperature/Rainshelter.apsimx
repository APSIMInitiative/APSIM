                        "        [Description(\"Allow irrigation to be applied?\")]",
                        "        public bool IrrFactor { get; set; }",
                        "        [Description(\"Amount of fertiliser to be applied (kg/ha)\")]",
                        "        public double NitFactor { get; set; }",
                        "",
                        "        [EventSubscribe(\"StartOfSimulation\")]",
