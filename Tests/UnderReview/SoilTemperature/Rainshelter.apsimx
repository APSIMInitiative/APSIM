                    {
                      "$type": "Models.Manager, Models",
                      "CodeArray": [
                        "using Models.Soils;",
                        "using Models.Climate;",
                        "using System;",
                        "using Models.Core;",
                        "using APSIM.Shared.Utilities;",
                        "using System.Collections.Generic;",
                        "using System.Text;",
                        "using Models.PMF;",
                        "using Models;",
                        "using System.Xml.Serialization;",
                        "using System.Globalization;",
                        "using Newtonsoft.Json;",
                        "",
                        "// This script simulates a rainshelter, it controls the amount of rainfall (=0.0) and radiation (=95% of weather)",
                        "// It does that for a period between two dates, as well as at sporadic dates",
                        "",
                        "namespace Models",
                        "{",
                        "    [Serializable] ",
                        "    public class Script : Model",
                        "    {",
                        "        [Link] Weather Met;",
                        "        [Link] Clock Clock;",
                        "    ",
                        "        [Description(\"Start date: \")]",
                        "        public DateTime StartDate { get; set; }",
                        "        [Description(\"End date: \")]",
                        "        public DateTime EndDate { get; set; }",
                        "         [Description(\"Other dates: \")]",
                        "         public string OtherDateString  { get; set; }",
                        "         private const string dateFormat = \"dd/MM/yyyy\";",
                        "         [NonSerialized]",
                        "         private CultureInfo provider = CultureInfo.InvariantCulture;",
                        "",
                        "         [JsonIgnore] public string RainshelterCover  { get; set; }",
                        "",
                        "         private DateTime[] OtherDates;",
                        "         private double todaysRain;",
                        "",
                        "        [EventSubscribe(\"StartOfSimulation\")]",
                        "        private void OnSimulationCommencing(object sender, EventArgs e)",
                        "        {",
                        "            provider = CultureInfo.InvariantCulture;",
                        "            //gather the dates the variables will be controlled",
                        "            string[] auxDates = OtherDateString.Split(new Char []{' ',','});",
                        "            OtherDates = new DateTime[auxDates.Length];",
                        "            for (int i = 0; i < auxDates.Length; i++)",
                        "                OtherDates[i] = DateTime.ParseExact(auxDates[i], dateFormat, provider);",
                        "",
                        "            RainshelterCover = \"Off\";",
                        "        }",
                        "",
                        "        [EventSubscribe(\"PreparingNewWeatherData\")]",
                        "        private void OnPreparingNewWeatherData(object sender, EventArgs e)",
                        "        {",
                        "            RainshelterCover = \"Off\";",
                        "",
                        "            if ((Clock.Today.Date >= StartDate.Date) && (Clock.Today.Date <= EndDate.Date))",
                        "            {",
                        "                todaysRain = Met.Rain;",
                        "                changeMetData();",
                        "            }",
                        "            else",
                        "            {",
                        "                for (int i = 0; i < OtherDates.Length; i++)",
                        "                    if (Clock.Today.Date == OtherDates[0].Date)",
                        "                        changeMetData();",
                        "            }",
                        "",
                        "            // rain shelter partial day cover",
                        "            if (Clock.Today.Date == DateTime.ParseExact(\"01/09/2017\", dateFormat, provider).Date)",
                        "                Met.Rain -= 2.0;",
                        "            if (Clock.Today.Date == DateTime.ParseExact(\"08/11/2017\", dateFormat, provider).Date)",
                        "                Met.Rain -= 0.3;",
                        "",
                        "            // rain shelter cover removed for weekend",
                        "            if ((Clock.Today.Date == DateTime.ParseExact(\"03/06/2017\", dateFormat, provider).Date)||(Clock.Today.Date == DateTime.ParseExact(\"04/06/2017\", dateFormat, provider).Date))",
                        "            {",
                        "                Met.Rain = todaysRain;",
                        "                //Met.Radn /= 0.95;",
                        "                RainshelterCover = \"Off\";",
                        "            }",
                        "        }",
                        "",
                        "        private void changeMetData()",
                        "        {",
                        "            Met.Rain *= 0.0;",
                        "            //Met.Radn *= 0.95;",
                        "            RainshelterCover = \"On\";",
                        "        }",
                        "    }",
                        "}"
