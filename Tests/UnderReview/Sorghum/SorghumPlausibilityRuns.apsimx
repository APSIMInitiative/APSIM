                      "$type": "Models.Manager, Models",
                      "Code": "using Models.Soils;\r\nusing System.Xml.Serialization;\r\nusing Models;\r\nusing Models.PMF;\r\nusing System.Text;\r\nusing System.Collections.Generic;\r\nusing APSIM.Shared.Utilities;\r\nusing Models.Core;\r\nusing System;\r\nusing Models.Climate;\r\n\r\nnamespace Models\r\n{\r\n    [Serializable] \r\n    [System.Xml.Serialization.XmlInclude(typeof(Model))]\r\n    public class Script : Model\r\n    {\r\n        [Link] Weather Met;\r\n        [Link] Clock Clock;\r\n        [Link] Zone zone;\r\n    \r\n        [Description(\"CO2 conc\")]\r\n        public double CO2 { get; set; }\r\n    \r\n        \r\n        [EventSubscribe(\"PreparingNewWeatherData\")]\r\n        private void OnPreparingNewWeatherData(object sender, EventArgs e)\r\n        {\r\n            zone.Set(\"Weather.CO2\", CO2);\r\n        }\r\n    }\r\n}\r\n",
