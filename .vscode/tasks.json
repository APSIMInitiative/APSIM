{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "debug clean",
            "command": "dotnet",
            "type": "process",
            "args": [
                "clean",
                "--verbosity",
                "minimal"
            ]
        },
        {
            "label": "debug build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/ApsimX.sln",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary",
                "--verbosity",
                "minimal"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label":"debug",
            "dependsOrder": "sequence",
            "dependsOn":[
                "debug clean",
                "debug build"
            ]
        }
    ]
}