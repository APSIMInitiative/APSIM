<?xml version="1.0" encoding="UTF-8"?>
<interface>
	<requires lib="gtk+" version="2.24"/>
	<!-- interface-naming-policy project-wide -->
	<object class="GtkPaned" id="main_widget">
		<property name="orientation">vertical</property>
		<property name="visible">True</property>
		<property name="can_focus">False</property>
		<child>
			<object class="GtkPaned" id="top_paned">
				<property name="orientation">horizontal</property>
				<property name="visible">True</property>
				<property name="can_focus">True</property>
				<child>
					<object class="GtkBox" id="chart_box">
						<property name="orientation">horizontal</property>
						<property name="visible">True</property>
						<property name="can_focus">True</property>
					</object>
					<packing>
						<property name="position">0</property>
					</packing>
				</child>
				<child>
					<object class="GtkBox" id="instructions_box">
						<property name="orientation">horizontal</property>
						<property name="visible">True</property>
						<property name="can_focus">True</property>
					</object>
					<packing>
						<property name="position">1</property>
					</packing>
				</child>
			</object>
			
		</child>
		<child>
			<object class="GtkPaned" id="settings_paned">
				<property name="orientation">horizontal</property>
				<property name="visible">True</property>
				<property name="can_focus">True</property>
				<child>
					<object class="GtkBox" id="properties_box">
						<property name="orientation">horizontal</property>
						<property name="visible">True</property>
						<property name="can_focus">True</property>
					</object>
				</child>
				<child>
					<object class="GtkBox" id="object_box">
						<property name="orientation">horizontal</property>
						<property name="visible">True</property>
						<property name="can_focus">True</property>
					</object>
				</child>
			</object>
		</child>
	</object>
</interface>