<?xml version="1.0" encoding="Windows-1252"?>
<Nutrient>
  <Name>Nutrient</Name>
  <Memo>
    <Name>TitlePage</Name>
    <IncludeInDocumentation>true</IncludeInDocumentation>
    <MemoText>
      <![CDATA[
# The APSIM Soil Nutrient Model

The APSIM soil nutrient model has been developed to simulate soil organic matter pools and the flows and losses of soil nutrients.  This model captures the functionality previously provided in the APSIM SoilN model ([probert_apsims_1998]).

The model consists of:

* a series of pools to capture soil organic matter types
* pools for mineral N including nitrate, ammonium and urea.
]]>
    </MemoText>
  </Memo>
  <NutrientPool>
    <Name>Inert</Name>
    <Memo>
      <Name>Memo</Name>
      <IncludeInDocumentation>true</IncludeInDocumentation>
      <MemoText><![CDATA[The inert pool captures organic matter that either does not decompose, or decomposes at a rate slow enough to be of little importance to simulations.  Such as approach was also captured in the APSIM SoilN model ([probert_apsims_1998]).]]></MemoText>
    </Memo>
    <ExpressionFunction>
      <Name>InitialCarbon</Name>
      <IncludeInDocumentation>true</IncludeInDocumentation>
      <Expression>[Soil].FInert*[Soil].OC * 100 * [Soil].BD * [Soil].Thickness</Expression>
    </ExpressionFunction>
    <ExpressionFunction>
      <Name>InitialNitrogen</Name>
      <IncludeInDocumentation>true</IncludeInDocumentation>
      <Expression>[Inert].C / [Soil].SoilOrganicMatter.SoilCN</Expression>
    </ExpressionFunction>
    <IncludeInDocumentation>true</IncludeInDocumentation>
  </NutrientPool>
  <NutrientPool>
    <Name>SurfaceResidue</Name>
    <Constant>
      <Name>InitialCarbon</Name>
      <IncludeInDocumentation>true</IncludeInDocumentation>
      <FixedValue>0</FixedValue>
    </Constant>
    <Constant>
      <Name>InitialNitrogen</Name>
      <IncludeInDocumentation>true</IncludeInDocumentation>
      <FixedValue>0</FixedValue>
    </Constant>
    <CarbonFlow>
      <Name>Decomposition</Name>
      <Constant>
        <Name>Rate</Name>
        <IncludeInDocumentation>true</IncludeInDocumentation>
        <FixedValue>1</FixedValue>
      </Constant>
      <Constant>
        <Name>CO2Efficiency</Name>
        <IncludeInDocumentation>true</IncludeInDocumentation>
        <FixedValue>0.4</FixedValue>
      </Constant>
      <Constant>
        <Name>N2OFraction</Name>
        <IncludeInDocumentation>true</IncludeInDocumentation>
        <FixedValue>0</FixedValue>
      </Constant>
      <IncludeInDocumentation>true</IncludeInDocumentation>
      <destinationNames>
        <string>Microbial</string>
        <string>Humic</string>
      </destinationNames>
      <destinationFraction>
        <double>0.9</double>
        <double>0.1</double>
      </destinationFraction>
    </CarbonFlow>
    <IncludeInDocumentation>true</IncludeInDocumentation>
  </NutrientPool>
  <Solute>
    <Name>Urea</Name>
    <NFlow>
      <Name>Hydrolysis</Name>
      <Constant>
        <Name>NLoss</Name>
        <IncludeInDocumentation>true</IncludeInDocumentation>
        <FixedValue>0</FixedValue>
      </Constant>
      <MultiplyFunction>
        <Name>Rate</Name>
        <BoundFunction>
          <Name>PotentialRate</Name>
          <Constant>
            <Name>Upper</Name>
            <IncludeInDocumentation>true</IncludeInDocumentation>
            <FixedValue>1</FixedValue>
          </Constant>
          <Constant>
            <Name>Lower</Name>
            <IncludeInDocumentation>true</IncludeInDocumentation>
            <FixedValue>0.25</FixedValue>
          </Constant>
          <ExpressionFunction>
            <Name>Rate</Name>
            <IncludeInDocumentation>true</IncludeInDocumentation>
            <Expression> -1.12 + 1.31 * [Soil].OC + 0.203 * [Soil].PH - 0.155 * [Soil].OC * [Soil].PH</Expression>
          </ExpressionFunction>
          <IncludeInDocumentation>true</IncludeInDocumentation>
        </BoundFunction>
        <MinimumFunction>
          <Name>PNI</Name>
          <BoundFunction>
            <Name>WF</Name>
            <Constant>
              <Name>Upper</Name>
              <IncludeInDocumentation>true</IncludeInDocumentation>
              <FixedValue>1</FixedValue>
            </Constant>
            <Constant>
              <Name>Lower</Name>
              <IncludeInDocumentation>true</IncludeInDocumentation>
              <FixedValue>0</FixedValue>
            </Constant>
            <ExpressionFunction>
              <Name>Wfactor</Name>
              <IncludeInDocumentation>true</IncludeInDocumentation>
              <Expression>[WaterFactorForMineralisation]+0.2</Expression>
            </ExpressionFunction>
            <IncludeInDocumentation>true</IncludeInDocumentation>
          </BoundFunction>
          <BoundFunction>
            <Name>TF</Name>
            <Constant>
              <Name>Upper</Name>
              <IncludeInDocumentation>true</IncludeInDocumentation>
              <FixedValue>1</FixedValue>
            </Constant>
            <Constant>
              <Name>Lower</Name>
              <IncludeInDocumentation>true</IncludeInDocumentation>
              <FixedValue>0</FixedValue>
            </Constant>
            <ExpressionFunction>
              <Name>Tfactor</Name>
              <IncludeInDocumentation>true</IncludeInDocumentation>
              <Expression>([Soil].Temperature/40)+0.2</Expression>
            </ExpressionFunction>
            <IncludeInDocumentation>true</IncludeInDocumentation>
          </BoundFunction>
          <IncludeInDocumentation>true</IncludeInDocumentation>
        </MinimumFunction>
        <IncludeInDocumentation>true</IncludeInDocumentation>
      </MultiplyFunction>
      <Constant>
        <Name>N2OFraction</Name>
        <IncludeInDocumentation>true</IncludeInDocumentation>
        <FixedValue>0</FixedValue>
      </Constant>
      <IncludeInDocumentation>true</IncludeInDocumentation>
      <sourceName>Urea</sourceName>
      <destinationName>NH4</destinationName>
    </NFlow>
    <IncludeInDocumentation>true</IncludeInDocumentation>
  </Solute>
  <Solute>
    <Name>NO3</Name>
    <NFlow>
      <Name>Denitrification</Name>
      <Memo>
        <Name>Memo</Name>
        <IncludeInDocumentation>true</IncludeInDocumentation>
        <MemoText>
          <![CDATA[References: 

Rolston DE, Rao PSC, Davidson JM, Jessup RE (1984). "Simulation of denitrification losses of Nitrate fertiliser applied to uncropped, cropped, and manure-amended field plots". Soil Science Vol 137, No 4, pp 270-278.
             
Reference for Carbon availability factor: Reddy KR, Khaleel R, Overcash MR (). "Carbon transformations in land areas receiving organic wastes in relation to nonpoint source pollution: A conceptual model".  J.Environ. Qual. 9:434-442.]]>
        </MemoText>
      </Memo>
      <Constant>
        <Name>NLoss</Name>
        <IncludeInDocumentation>true</IncludeInDocumentation>
        <FixedValue>1</FixedValue>
      </Constant>
      <MultiplyFunction>
        <Name>Rate</Name>
        <Constant>
          <Name>Constant</Name>
          <IncludeInDocumentation>true</IncludeInDocumentation>
          <FixedValue>0.0006</FixedValue>
        </Constant>
        <ExpressionFunction>
          <Name>ActiveC</Name>
          <ExpressionFunction>
            <Name>TotalCppm</Name>
            <IncludeInDocumentation>true</IncludeInDocumentation>
            <Expression>([Humic].C+[Inert].C+[FOMCarbohydrate].C+[FOMCellulose].C+[FOMLignin].C)/[ppm2kgha]</Expression>
          </ExpressionFunction>
          <IncludeInDocumentation>true</IncludeInDocumentation>
          <Expression>0.0031 * [TotalCppm] + 24.5</Expression>
        </ExpressionFunction>
        <BoundFunction>
          <Name>TF</Name>
          <Constant>
            <Name>Upper</Name>
            <IncludeInDocumentation>true</IncludeInDocumentation>
            <FixedValue>1</FixedValue>
          </Constant>
          <Constant>
            <Name>Lower</Name>
            <IncludeInDocumentation>true</IncludeInDocumentation>
            <FixedValue>0</FixedValue>
          </Constant>
          <ExpressionFunction>
            <Name>Tfactor</Name>
            <IncludeInDocumentation>true</IncludeInDocumentation>
            <Expression>0.1 * Exp(0.046 * [Soil].Temperature)</Expression>
          </ExpressionFunction>
          <IncludeInDocumentation>true</IncludeInDocumentation>
        </BoundFunction>
        <LinearInterpolationFunction>
          <Name>WF</Name>
          <SoilWaterScale>
            <Name>SoilWaterScale</Name>
            <IncludeInDocumentation>true</IncludeInDocumentation>
          </SoilWaterScale>
          <XYPairs>
            <Name>XYPairs</Name>
            <IncludeInDocumentation>true</IncludeInDocumentation>
            <X>
              <double>0</double>
              <double>1</double>
              <double>2</double>
            </X>
            <Y>
              <double>0</double>
              <double>0</double>
              <double>1</double>
            </Y>
          </XYPairs>
          <IncludeInDocumentation>true</IncludeInDocumentation>
          <XProperty>[SoilWaterScale]</XProperty>
        </LinearInterpolationFunction>
        <IncludeInDocumentation>true</IncludeInDocumentation>
      </MultiplyFunction>
      <ExpressionFunction>
        <Name>N2OFraction</Name>
        <MinimumFunction>
          <Name>N2N2ORatio</Name>
          <Constant>
            <Name>Zero</Name>
            <IncludeInDocumentation>true</IncludeInDocumentation>
            <FixedValue>1</FixedValue>
          </Constant>
          <MultiplyFunction>
            <Name>Ratio</Name>
            <Constant>
              <Name>Constant</Name>
              <IncludeInDocumentation>true</IncludeInDocumentation>
              <FixedValue>25.1</FixedValue>
            </Constant>
            <LinearInterpolationFunction>
              <Name>WFPSEffect</Name>
              <XYPairs>
                <Name>XYPairs</Name>
                <IncludeInDocumentation>true</IncludeInDocumentation>
                <X>
                  <double>0</double>
                  <double>28</double>
                  <double>88</double>
                  <double>100</double>
                </X>
                <Y>
                  <double>0.1</double>
                  <double>0.1</double>
                  <double>1</double>
                  <double>1.18</double>
                </Y>
              </XYPairs>
              <IncludeInDocumentation>true</IncludeInDocumentation>
              <XProperty>[SoilWaterScale]</XProperty>
            </LinearInterpolationFunction>
            <MinimumFunction>
              <Name>CO2Effect</Name>
              <Constant>
                <Name>Constant</Name>
                <IncludeInDocumentation>true</IncludeInDocumentation>
                <FixedValue>0.16</FixedValue>
              </Constant>
              <ExpressionFunction>
                <Name>Effect</Name>
                <IncludeInDocumentation>true</IncludeInDocumentation>
                <Expression>Math.Exp(-0.8 * [Nutrient].NO3.kgha / [Nutrient].Catm)</Expression>
              </ExpressionFunction>
              <IncludeInDocumentation>true</IncludeInDocumentation>
            </MinimumFunction>
            <IncludeInDocumentation>true</IncludeInDocumentation>
          </MultiplyFunction>
          <IncludeInDocumentation>true</IncludeInDocumentation>
        </MinimumFunction>
        <IncludeInDocumentation>true</IncludeInDocumentation>
        <Expression>1 / ([N2N2ORatio].Value() + 1)</Expression>
      </ExpressionFunction>
      <IncludeInDocumentation>true</IncludeInDocumentation>
      <sourceName>NO3</sourceName>
    </NFlow>
    <IncludeInDocumentation>true</IncludeInDocumentation>
  </Solute>
  <NutrientPool>
    <Name>Microbial</Name>
    <ExpressionFunction>
      <Name>InitialCarbon</Name>
      <IncludeInDocumentation>true</IncludeInDocumentation>
      <Expression>(([Soil].OC * 100 * [Soil].BD * [Soil].Thickness -[Inert].C) * [Soil].FBiom) / (1 + [Soil].FBiom)</Expression>
    </ExpressionFunction>
    <ExpressionFunction>
      <Name>InitialNitrogen</Name>
      <IncludeInDocumentation>true</IncludeInDocumentation>
      <Expression>[Microbial].C /  [CNRatio]</Expression>
    </ExpressionFunction>
    <Constant>
      <Name>CNRatio</Name>
      <IncludeInDocumentation>true</IncludeInDocumentation>
      <FixedValue>8</FixedValue>
    </Constant>
    <CarbonFlow>
      <Name>Decomposition</Name>
      <MultiplyFunction>
        <Name>Rate</Name>
        <Constant>
          <Name>PotentialRate</Name>
          <IncludeInDocumentation>true</IncludeInDocumentation>
          <FixedValue>0.0081</FixedValue>
        </Constant>
        <VariableReference>
          <Name>TF</Name>
          <IncludeInDocumentation>true</IncludeInDocumentation>
          <VariableName>[TemperatureFactorForMineralisation]</VariableName>
        </VariableReference>
        <VariableReference>
          <Name>WF</Name>
          <IncludeInDocumentation>true</IncludeInDocumentation>
          <VariableName>[WaterFactorForMineralisation]</VariableName>
        </VariableReference>
        <IncludeInDocumentation>true</IncludeInDocumentation>
      </MultiplyFunction>
      <Constant>
        <Name>CO2Efficiency</Name>
        <IncludeInDocumentation>true</IncludeInDocumentation>
        <FixedValue>0.4</FixedValue>
      </Constant>
      <IncludeInDocumentation>true</IncludeInDocumentation>
      <destinationNames>
        <string>Microbial</string>
        <string>Humic</string>
      </destinationNames>
      <destinationFraction>
        <double>0.6</double>
        <double>0.4</double>
      </destinationFraction>
    </CarbonFlow>
    <IncludeInDocumentation>true</IncludeInDocumentation>
  </NutrientPool>
  <NutrientPool>
    <Name>Humic</Name>
    <ExpressionFunction>
      <Name>InitialCarbon</Name>
      <IncludeInDocumentation>true</IncludeInDocumentation>
      <Expression>(1-[Soil].FInert)*[Soil].OC * 100 * [Soil].BD * [Soil].Thickness - [Microbial].C</Expression>
    </ExpressionFunction>
    <ExpressionFunction>
      <Name>InitialNitrogen</Name>
      <IncludeInDocumentation>true</IncludeInDocumentation>
      <Expression>[Humic].C / [Soil].SoilOrganicMatter.SoilCN</Expression>
    </ExpressionFunction>
    <CarbonFlow>
      <Name>Decomposition</Name>
      <MultiplyFunction>
        <Name>Rate</Name>
        <Constant>
          <Name>PotentialRate</Name>
          <IncludeInDocumentation>true</IncludeInDocumentation>
          <FixedValue>0.00015</FixedValue>
        </Constant>
        <VariableReference>
          <Name>TF</Name>
          <IncludeInDocumentation>true</IncludeInDocumentation>
          <VariableName>[TemperatureFactorForMineralisation]</VariableName>
        </VariableReference>
        <VariableReference>
          <Name>WF</Name>
          <IncludeInDocumentation>true</IncludeInDocumentation>
          <VariableName>[WaterFactorForMineralisation]</VariableName>
        </VariableReference>
        <IncludeInDocumentation>true</IncludeInDocumentation>
      </MultiplyFunction>
      <Constant>
        <Name>CO2Efficiency</Name>
        <IncludeInDocumentation>true</IncludeInDocumentation>
        <FixedValue>0.4</FixedValue>
      </Constant>
      <IncludeInDocumentation>true</IncludeInDocumentation>
      <destinationNames>
        <string>Microbial</string>
      </destinationNames>
      <destinationFraction>
        <double>1</double>
      </destinationFraction>
    </CarbonFlow>
    <IncludeInDocumentation>true</IncludeInDocumentation>
  </NutrientPool>
  <NutrientPool>
    <Name>FOMLignin</Name>
    <ExpressionFunction>
      <Name>InitialCarbon</Name>
      <IncludeInDocumentation>true</IncludeInDocumentation>
      <Expression>[Soil].InitialRootWt * 0.1 * 0.4</Expression>
    </ExpressionFunction>
    <ExpressionFunction>
      <Name>InitialNitrogen</Name>
      <IncludeInDocumentation>true</IncludeInDocumentation>
      <Expression>[FOMLignin].C / [Soil].SoilOrganicMatter.RootCN</Expression>
    </ExpressionFunction>
    <CarbonFlow>
      <Name>Decomposition</Name>
      <MultiplyFunction>
        <Name>Rate</Name>
        <Constant>
          <Name>PotentialRate</Name>
          <IncludeInDocumentation>true</IncludeInDocumentation>
          <FixedValue>0.0095</FixedValue>
        </Constant>
        <VariableReference>
          <Name>TF</Name>
          <IncludeInDocumentation>true</IncludeInDocumentation>
          <VariableName>[TemperatureFactorForMineralisation]</VariableName>
        </VariableReference>
        <VariableReference>
          <Name>WF</Name>
          <IncludeInDocumentation>true</IncludeInDocumentation>
          <VariableName>[WaterFactorForMineralisation]</VariableName>
        </VariableReference>
        <VariableReference>
          <Name>CNRF</Name>
          <IncludeInDocumentation>true</IncludeInDocumentation>
          <VariableName>[CNRatioFactorForMineralisation]</VariableName>
        </VariableReference>
        <IncludeInDocumentation>true</IncludeInDocumentation>
      </MultiplyFunction>
      <Constant>
        <Name>CO2Efficiency</Name>
        <IncludeInDocumentation>true</IncludeInDocumentation>
        <FixedValue>0.4</FixedValue>
      </Constant>
      <IncludeInDocumentation>true</IncludeInDocumentation>
      <destinationNames>
        <string>Microbial</string>
        <string>Humic</string>
      </destinationNames>
      <destinationFraction>
        <double>0.9</double>
        <double>0.1</double>
      </destinationFraction>
    </CarbonFlow>
    <IncludeInDocumentation>true</IncludeInDocumentation>
  </NutrientPool>
  <NutrientPool>
    <Name>FOMCellulose</Name>
    <ExpressionFunction>
      <Name>InitialCarbon</Name>
      <IncludeInDocumentation>true</IncludeInDocumentation>
      <Expression>[Soil].InitialRootWt * 0.7 * 0.4</Expression>
    </ExpressionFunction>
    <ExpressionFunction>
      <Name>InitialNitrogen</Name>
      <IncludeInDocumentation>true</IncludeInDocumentation>
      <Expression>[FOMCellulose].C / [Soil].SoilOrganicMatter.RootCN</Expression>
    </ExpressionFunction>
    <CarbonFlow>
      <Name>Decomposition</Name>
      <MultiplyFunction>
        <Name>Rate</Name>
        <Constant>
          <Name>PotentialRate</Name>
          <IncludeInDocumentation>true</IncludeInDocumentation>
          <FixedValue>0.05</FixedValue>
        </Constant>
        <VariableReference>
          <Name>TF</Name>
          <IncludeInDocumentation>true</IncludeInDocumentation>
          <VariableName>[TemperatureFactorForMineralisation]</VariableName>
        </VariableReference>
        <VariableReference>
          <Name>WF</Name>
          <IncludeInDocumentation>true</IncludeInDocumentation>
          <VariableName>[WaterFactorForMineralisation]</VariableName>
        </VariableReference>
        <VariableReference>
          <Name>CNRF</Name>
          <IncludeInDocumentation>true</IncludeInDocumentation>
          <VariableName>[CNRatioFactorForMineralisation]</VariableName>
        </VariableReference>
        <IncludeInDocumentation>true</IncludeInDocumentation>
      </MultiplyFunction>
      <Constant>
        <Name>CO2Efficiency</Name>
        <IncludeInDocumentation>true</IncludeInDocumentation>
        <FixedValue>0.4</FixedValue>
      </Constant>
      <IncludeInDocumentation>true</IncludeInDocumentation>
      <destinationNames>
        <string>Microbial</string>
        <string>Humic</string>
      </destinationNames>
      <destinationFraction>
        <double>0.9</double>
        <double>0.1</double>
      </destinationFraction>
    </CarbonFlow>
    <IncludeInDocumentation>true</IncludeInDocumentation>
  </NutrientPool>
  <NutrientPool>
    <Name>FOMCarbohydrate</Name>
    <ExpressionFunction>
      <Name>InitialCarbon</Name>
      <IncludeInDocumentation>true</IncludeInDocumentation>
      <Expression>[Soil].InitialRootWt * 0.2 * 0.4</Expression>
    </ExpressionFunction>
    <ExpressionFunction>
      <Name>InitialNitrogen</Name>
      <IncludeInDocumentation>true</IncludeInDocumentation>
      <Expression>[FOMCarbohydrate].C / [Soil].SoilOrganicMatter.RootCN</Expression>
    </ExpressionFunction>
    <CarbonFlow>
      <Name>Decomposition</Name>
      <MultiplyFunction>
        <Name>Rate</Name>
        <Constant>
          <Name>PotentialRate</Name>
          <IncludeInDocumentation>true</IncludeInDocumentation>
          <FixedValue>0.2</FixedValue>
        </Constant>
        <VariableReference>
          <Name>TF</Name>
          <IncludeInDocumentation>true</IncludeInDocumentation>
          <VariableName>[TemperatureFactorForMineralisation]</VariableName>
        </VariableReference>
        <VariableReference>
          <Name>WF</Name>
          <IncludeInDocumentation>true</IncludeInDocumentation>
          <VariableName>[WaterFactorForMineralisation]</VariableName>
        </VariableReference>
        <VariableReference>
          <Name>CNRF</Name>
          <IncludeInDocumentation>true</IncludeInDocumentation>
          <VariableName>[CNRatioFactorForMineralisation]</VariableName>
        </VariableReference>
        <IncludeInDocumentation>true</IncludeInDocumentation>
      </MultiplyFunction>
      <Constant>
        <Name>CO2Efficiency</Name>
        <IncludeInDocumentation>true</IncludeInDocumentation>
        <FixedValue>0.4</FixedValue>
      </Constant>
      <IncludeInDocumentation>true</IncludeInDocumentation>
      <destinationNames>
        <string>Microbial</string>
        <string>Humic</string>
      </destinationNames>
      <destinationFraction>
        <double>0.9</double>
        <double>0.1</double>
      </destinationFraction>
    </CarbonFlow>
    <IncludeInDocumentation>true</IncludeInDocumentation>
  </NutrientPool>
  <Solute>
    <Name>NH4</Name>
    <NFlow>
      <Name>Nitrification</Name>
      <Constant>
        <Name>NLoss</Name>
        <IncludeInDocumentation>true</IncludeInDocumentation>
        <FixedValue>0</FixedValue>
      </Constant>
      <MultiplyFunction>
        <Name>Rate</Name>
        <ExpressionFunction>
          <Name>MichaelisMenton</Name>
          <IncludeInDocumentation>true</IncludeInDocumentation>
          <Expression>40/([Nutrient].NH4.ppm+90)</Expression>
        </ExpressionFunction>
        <MinimumFunction>
          <Name>PNI</Name>
          <LinearInterpolationFunction>
            <Name>WF</Name>
            <SoilWaterScale>
              <Name>SoilWaterScale</Name>
              <IncludeInDocumentation>true</IncludeInDocumentation>
            </SoilWaterScale>
            <XYPairs>
              <Name>XYPairs</Name>
              <IncludeInDocumentation>true</IncludeInDocumentation>
              <X>
                <double>0</double>
                <double>0.25</double>
                <double>1</double>
                <double>2</double>
              </X>
              <Y>
                <double>0</double>
                <double>1</double>
                <double>1</double>
                <double>0</double>
              </Y>
            </XYPairs>
            <IncludeInDocumentation>true</IncludeInDocumentation>
            <XProperty>[SoilWaterScale]</XProperty>
          </LinearInterpolationFunction>
          <VariableReference>
            <Name>TF</Name>
            <IncludeInDocumentation>true</IncludeInDocumentation>
            <VariableName>[TemperatureFactorForMineralisation]</VariableName>
          </VariableReference>
          <LinearInterpolationFunction>
            <Name>pHF</Name>
            <XYPairs>
              <Name>XYPairs</Name>
              <IncludeInDocumentation>true</IncludeInDocumentation>
              <X>
                <double>4.5</double>
                <double>6</double>
                <double>8</double>
                <double>9</double>
              </X>
              <Y>
                <double>0</double>
                <double>1</double>
                <double>1</double>
                <double>0</double>
              </Y>
            </XYPairs>
            <IncludeInDocumentation>true</IncludeInDocumentation>
            <XProperty>[Soil].PH</XProperty>
          </LinearInterpolationFunction>
          <IncludeInDocumentation>true</IncludeInDocumentation>
        </MinimumFunction>
        <IncludeInDocumentation>true</IncludeInDocumentation>
      </MultiplyFunction>
      <Constant>
        <Name>N2OFraction</Name>
        <IncludeInDocumentation>true</IncludeInDocumentation>
        <FixedValue>0</FixedValue>
      </Constant>
      <IncludeInDocumentation>true</IncludeInDocumentation>
      <sourceName>NH4</sourceName>
      <destinationName>NO3</destinationName>
    </NFlow>
    <IncludeInDocumentation>true</IncludeInDocumentation>
  </Solute>
  <NutrientPool>
    <Name>Biochar</Name>
    <Constant>
      <Name>InitialCarbon</Name>
      <IncludeInDocumentation>true</IncludeInDocumentation>
      <FixedValue>0</FixedValue>
    </Constant>
    <Constant>
      <Name>InitialNitrogen</Name>
      <IncludeInDocumentation>true</IncludeInDocumentation>
      <FixedValue>0</FixedValue>
    </Constant>
    <IncludeInDocumentation>true</IncludeInDocumentation>
  </NutrientPool>
  <MinimumFunction>
    <Name>TemperatureFactorForMineralisation</Name>
    <ExpressionFunction>
      <Name>TF</Name>
      <Constant>
        <Name>Topt</Name>
        <IncludeInDocumentation>true</IncludeInDocumentation>
        <FixedValue>32</FixedValue>
      </Constant>
      <MaximumFunction>
        <Name>AbsT</Name>
        <Constant>
          <Name>Zero</Name>
          <IncludeInDocumentation>true</IncludeInDocumentation>
          <FixedValue>0</FixedValue>
        </Constant>
        <VariableReference>
          <Name>VariableReference</Name>
          <IncludeInDocumentation>true</IncludeInDocumentation>
          <VariableName>[Soil].Temperature</VariableName>
        </VariableReference>
        <IncludeInDocumentation>true</IncludeInDocumentation>
      </MaximumFunction>
      <IncludeInDocumentation>true</IncludeInDocumentation>
      <Expression>([AbsT]^2)/([Topt]^2)</Expression>
    </ExpressionFunction>
    <Constant>
      <Name>One</Name>
      <IncludeInDocumentation>true</IncludeInDocumentation>
      <FixedValue>1</FixedValue>
    </Constant>
    <IncludeInDocumentation>true</IncludeInDocumentation>
  </MinimumFunction>
    <LinearInterpolationFunction>
      <Name>WaterFactorForMineralisation</Name>
      <SoilWaterScale>
        <Name>SoilWaterScale</Name>
        <IncludeInDocumentation>true</IncludeInDocumentation>
      </SoilWaterScale>
      <XYPairs>
        <Name>XYPairs</Name>
        <IncludeInDocumentation>true</IncludeInDocumentation>
        <X>
          <double>0</double>
